<div class="section mb-[64px] video_feed">
  {% assign title_text = section.settings.section_title %}
  {% assign title_image = section.settings.title_image %}
  {% include 'section_title' with title_text, title_image %}

  <div class="swiper video_feed_swiper">
    <div class="swiper-wrapper">
      {% for block in section.blocks %}
        <div class="swiper-slide rounded-[32px] relative">
          <div class="video_feed_video w-full h-full">
            <div class="video_background"></div>
            <img
              class="w-full h-full object-cover rounded-[32px] absolute top-[0] left-[0]"
              src="{{ block.settings.video.preview_image.src | img_url}}"
              alt="">
            <div class="video_play_button video_play_gold"></div>
            <h2 class="close_video">close</h2>
          </div>
        </div>
      {% endfor %}
    </div>
    <div class="swiper-pagination"></div>
  </div>
</div>
